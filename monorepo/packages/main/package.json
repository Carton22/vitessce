{
  "name": "vitessce",
  "version": "1.0.0",
  "author": "Mark Keller",
  "homepage": "http://vitessce.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitessce/vitessce.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "build-lib": "npm run build-lib:prod && npm run build-lib:dev",
    "build-lib:prod": "APP_ENV=production vite build --mode lib",
    "build-lib:dev": "APP_ENV=development vite build --mode lib"
  },
  "dependencies": {
    "@vitessce/config": "workspace:*",
    "@vitessce/constants": "workspace:*",
    "@vitessce/my-component": "workspace:*",
    "@vitessce/utils": "workspace:*",
    "@vitessce/vit-s": "workspace:*"
  },
  "devDependencies": {
    "esbuild": "^0.15.10",
    "vite": "^3.0.0"
  }
}
