  "scripts": {
    "clean": "rm -r dist-demo dist",
    "prepublishOnly": "npm run build-lib:prod",
    "start": "rollup -c rollup.config.demo.js --environment NODE_ENV:development --watch",
    "build-demo:dev": "rollup -c rollup.config.demo.js --environment NODE_ENV:development",
    "build-demo:prod": "rollup -c rollup.config.demo.js --environment NODE_ENV:production",
    "build-lib:dev": "rollup -c rollup.config.lib.js --environment NODE_ENV:development",
    "build-lib:prod": "rollup -c rollup.config.lib.js --environment NODE_ENV:production",
    "build:prod": "npm run build-demo:prod && npm run build-lib:prod",
    "build:dev": "npm run build-demo:dev && npm run build-lib:dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint-fix": "eslint -c .eslintrc.yml --ext .js --fix src/",
    "postinstall": "patch-package"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
