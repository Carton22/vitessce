{
  "name": "vitessce",
  "version": "1.2.2",
  "description": "Vitessce app and React component library",
  "author": "Gehlenborg Lab",
  "homepage": "http://vitessce.io/",
  "repository": "https://github.com/vitessce/vitessce/",
  "main": "index.js",
  "scripts": {
    "start": "pnpm -r --parallel start",
    "start-demo": "pnpm -r --parallel --filter=!docs start",
    "start-docs": "pnpm -r --parallel --filter=!demo start",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "format-check": "prettier --check --ignore-path .gitignore sites/ packages/",
    "format": "pnpm run format-check -- --write",
    "lint": "pnpm run format-check && eslint \"packages/*/src/**/*\" \"sites/*/src/**/*\"",
    "clean": "pnpm -r exec -- rm -rf node_modules"
  },
  "devDependencies": {
    "@pnpm/meta-updater": "^0.0.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.16.0",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-canvas-mock": "^2.3.0",
    "jsdom": "^20.0.0",
    "jsdom-worker": "^0.3.0",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.55.0",
    "serve-static": "^1.15.0",
    "terser": "^5.15.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.23.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    }
  },
  "keywords": [
    "react-component"
  ],
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
