  "scripts": {
    "clean": "rm -r es lib umd css demo/dist",
    "prepublishOnly": "npm run build:lib",
    "start": "rollup -c rollup.config.demo.js --environment NODE_ENV:development --watch",
    "build:demo": "rollup -c rollup.config.demo.js --environment NODE_ENV:production",
    "build:lib:development": "rollup -c rollup.config.lib.js --environment NODE_ENV:development",
    "build:lib:production": "rollup -c rollup.config.lib.js --environment NODE_ENV:production",
    "build:lib": "npm run build:lib:development && npm run build:lib:production",
    "build": "npm run build:demo && npm run build:lib",
    "test": "karma start karma.config.js --single-run --no-auto-watch",
    "test:watch": "karma start karma.config.js --no-single-run --auto-watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint-fix": "eslint -c .eslintrc.yml --ext .js --fix src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
